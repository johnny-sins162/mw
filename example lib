local library = loadstring(game:HttpGet("https://pastebin.com/raw/1RJ2nAhW"))()

local menu = library:New()
local legit = menu:Tab("Legit")

local general = legit:Tab("General")
local pistol = legit:Tab("Pistol")
local smg = legit:Tab("SMG")
local rifle = legit:Tab("Rifle")
local sniper = legit:Tab("Sniper")

local aimbot = general:Section("Aimbot", "Left")
local silent = general:Section("Silent Aim", "Right")

aimbot:Cheat("Toggle", "Test Toggle", function(state)
    print("Test Toggle ->", state.Toggle)
end)
